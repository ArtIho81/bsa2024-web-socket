{
  "name": "http-ws-hw-starter",
  "version": "2.0.0",
  "description": "Starter for HTTP / WS homework. Binary Studio Academy",
  "type": "module",
  "engines": {
    "node": "20.x.x",
    "npm": "10.x.x"
  },
  "scripts": {
    "dev": "nodemon --exec \"node --loader ts-node/esm\" src/server.ts",
    "build": "tsc",
    "format:check": "npx prettier --check \"**/*.{ts,mjs,css,md,html}\"",
    "format:fix": "npx prettier --write \"**/*.{ts,mjs,css,md,html}\""
  },
  "keywords": [
    "bsa",
    "homework"
  ],
  "author": "Kyrylo Burmelov",
  "license": "ISC",
  "dependencies": {
    "@types/express": "4.17.21",
    "@types/node": "20.10.8",
    "express": "4.18.2",
    "socket.io": "4.7.3"
  },
  "devDependencies": {
    "nodemon": "3.0.2",
    "prettier": "3.1.1",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  }
}
